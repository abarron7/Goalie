

<div id="home">
  <div class="container">
    <div class="row">
      <div class="col-8 offset-2">
        <h1>{{msg}}</h1>
        <h2>This is our Home Page!</h2>
        <h3>New User</h3>
          <a href="newuser">Click here!</a>
        <br>
        <h3>Existing Users</h3>
        <ul id="financials-list" class="list-group"><ul>
            {{#each dbUsers}}
            <p>Username: <a href="users/{{this.id}}">{{this.username}}</a></p>
            <p>Goal: {{this.goaldescr}}</p>
            <p>Progress: {{this.goalprogress}}</p>
            <br>
            {{/each}}
          </ul>
        </div>
      </div>
    </div>
  </div>
</div>

